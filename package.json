{
  "name": "@3-shake/3design-ui",
  "version": "1.2.0",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "author": "locona <miyamae@3-shake.com>",
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3-shake/3design-ui.git"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build:types": "NODE_ENV=production tsc",
    "build:types:watch": "NODE_ENV=production tsc --watch",
    "build:js": "cp ./react-table-index.js ./node_modules/react-table/index.js && NODE_ENV=production rollup -c",
    "build:js:watch": "NODE_ENV=production rollup -c --watch",
    "build": "yarn build:types && yarn build:js",
    "lint": "eslint --ext .ts,.tsx,.cjs --ignore-pattern '**/__test*__/*' .",
    "lint-fix": "eslint --fix --ext .ts,.tsx,.cjs --ignore-pattern '**/__test*__/*' .",
    "storybook": "rm -rf node_modules/.cache/storybook && start-storybook --no-manager-cache -p 6006 -s public",
    "build-storybook": "build-storybook",
    "release": "shipjs prepare"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "5.0.0-alpha.34",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "5.0.0-alpha.34",
    "react-detect-offline": "^2.4.3",
    "react-dropzone": "^11.3.2",
    "react-spinners": "^0.11.0",
    "react-table": "^7.6.3",
    "react-tabs": "3.2.2",
    "twin.macro": "^2.2.3"
  },
  "devDependencies": {
    "shipjs": "0.23.3",
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@emotion/babel-plugin-jsx-pragmatic": "0.1.5",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@hookform/resolvers": "2.5.2",
    "@material-ui/types": "5.1.8",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-backgrounds": "6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/react": "6.2.9",
    "@types/react-table": "7.0.29",
    "@types/react-tabs": "2.3.2",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "autoprefixer": "10.2.6",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unused-imports": "1.1.1",
    "prettier": "2.3.1",
    "lint-staged": "11.0.0",
    "namor": "2.0.3",
    "postcss": "8.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.8.8",
    "rollup": "2.51.2",
    "rollup-config-analyzer": "2.2.0",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-terser": "7.0.2",
    "tailwindcss": "2.1.4",
    "ts-loader": "9.2.3",
    "typescript": "4.2.4",
    "yup": "0.32.9"
  },
  "peerDependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.3.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "tailwind.config.cjs"
  ],
  "babelMacros": {
    "twin": {
      "preset": "emotion",
      "config": "tailwind.config.cjs"
    }
  }
